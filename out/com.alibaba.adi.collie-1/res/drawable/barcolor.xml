<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="12.0dip" />
            <gradient android:startColor="#55ffffff" android:endColor="#55ffffff" android:angle="270.0" />
        </shape>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="12.0dip" />
                <gradient android:startColor="#ffff8800" android:endColor="#ffff8800" android:angle="270.0" />
            </shape>
        </clip>
    </item>
</layer-list>