<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="fill_parent" android:layout_height="fill_parent"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <ImageView android:id="@id/BgImageView" android:layout_width="fill_parent" android:layout_height="fill_parent" android:scaleType="centerCrop" />
    <include layout="@layout/layout_setting_banner" />
    <LinearLayout style="@style/setting_panel_wrap">
        <RelativeLayout style="@style/setting_activity_header">
            <Button android:id="@id/BackImageView" android:text="@string/about_eula" android:onClick="onClick" style="@style/setting_header_title" />
        </RelativeLayout>
        <com.alibaba.adi.collie.ui.view.BounceScrollView android:scrollbarThumbVertical="@drawable/scrollbar" android:id="@id/scroll" android:scrollbars="vertical" android:fillViewport="true" style="@style/setting_content_container">
            <TextView android:id="@id/detail" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/loading_asset_txt" style="@style/about_eula_content" />
        </com.alibaba.adi.collie.ui.view.BounceScrollView>
    </LinearLayout>
</RelativeLayout>